                                {# Fourth block INDEXES #}
                                <div class="row bg-success mb-1">
                                    <div class="col-xs-12">
                                        <b>INDEX / NAUWKEURIGHEID</b>
                                    </div>
                                </div>

                                <div class="row bg-white mb-1">
                                    <div class="col-xs-4"><b>Vaderindex</b></div>
                                    {% if not certificate.animals.c.indexes.indexFather.isEmpty %}
                                        <div class="col-xs-8">
                                            {% for output in certificate.animals.c.indexes.indexFather.stars_output %}
                                                {% if output == 'FULL' %}
                                                    <span><i class="fas fa-star"></i></span>
                                                {% elseif output == 'HALF' %}
                                                    <span><i class="fas fa-star-half-alt"></i></span>
                                                {% elseif output == 'EMPTY' %}
                                                    <span><i class="far fa-star"></i></span>
                                                {% endif %}
                                            {% endfor %}
                                            {{ '  '~certificate.animals.c.indexes.indexFather.value~' / '~certificate.animals.c.indexes.indexFather.accuracy~'%' }}
                                        </div>
                                    {% endif %}
                                </div>

                                <div class="row bg-white mb-1">
                                    <div class="col-xs-4"><b>Moederindex</b></div>
