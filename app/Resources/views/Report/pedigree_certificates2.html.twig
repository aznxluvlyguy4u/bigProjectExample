{% block body %}

    {% set fullStar = images_dir~'/baseline-star-24px.svg' %}
    {% set halfStar = images_dir~'/baseline-star_half-24px.svg' %}
    {% set emptyStar = images_dir~'/baseline-star_border-24px.svg' %}

    {#{% set fullStar = asset('assets/images/baseline-star-24px.svg') %}#}
    {#{% set halfStar = asset('assets/images/baseline-star_half-24px.svg') %}#}
    {#{% set emptyStar = asset('assets/images/baseline-star_border-24px.svg') %}#}

    {% for certificate in variables %}

        <div class="container mb-4 bg-gray">
            <div class="row py-4">
                <div class="col-xs-6">
                    <div class="pl-3 pr-2">

                        {# FIRST TAB#}
                        <div class="row">
                            <div class="col-xs-8">
                                <img class="img-responsive"  src="{{ images_dir~'/nsfo-logo@2x.png' }}" alt="">
                                <h1><b>AFSTAMMINGSBEWIJS</b></h1>
                            </div>

                            {#<div class="col-xs-8">#}
                                {#<img class="img-responsive"  src="{{ asset('assets/images/nsfo-logo@2x.png') }}" alt="">#}
                                {#<h1><b>AFSTAMMINGSBEWIJS</b></h1>#}
                            {#</div>#}

                            <div class="col-xs-12 bg-green rounded mb-5">
                                <div class="px-2">
                                    <div class="row">
                                        <div class="col-xs-12 py-1">
                                            <h2 class="display-inline"><b>DIER - </b></h2><h4 class="display-inline">{{ certificate.animals.c.section_type }}</h4>
                                        </div>
                                    </div>

                                    {# First block #}
                                    <div class="row bg-white mb-1">
                                        <div class="col-xs-3 fieldName text-nowrap">Uniek levensnr.</div>
                                        <div class="col-xs-9 fieldValue"><b>{{ certificate.animals.c.uln~'    (Oormerk)' }}</b></div>
                                    </div>

                                    <div class="row bg-white  mb-1">
                                        <div class="col-xs-3 fieldName">Stamboeknr.</div>
                                        <div class="col-xs-9 fieldValue"><b>{{ certificate.animals.c.pedigree }}</b></div>
                                    </div>

                                    <div class="row bg-white  mb-1">
                                        <div class="col-xs-3 fieldName">Naam</div>
                                        <div class="col-xs-9 fieldValue"><b>{{ certificate.animals.c.name }}</b></div>
                                    </div>

                                    <div class="row bg-white  mb-3">
                                        <div class="col-xs-3 fieldName">Diersoort</div>
                                        <div class="col-xs-9 fieldValue"><b>{{ certificate.animals.c.animalType }} ({{ certificate.animals.c.animalTypeInLatin }})</b></div>
                                    </div>

                                    {# Second block #}
                                    <div class="row mb-1">
                                        <div class="col-xs-6">
                                            <div class="pr-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName">Geslacht</div>
                                                    <div class="col-xs-6 fieldValue">{{ certificate.animals.c.gender }}</div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-6">
                                            <div class="pl-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName">Geboren als</div>
                                                    <div class="col-xs-6 fieldValue">{{ certificate.animals.c.nLing }}</div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row mb-1">
                                        <div class="col-xs-6">
                                            <div class="pr-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName">Ras</div>
                                                    <div class="col-xs-6 fieldValue">{{ certificate.animals.c.breedCode }}</div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-6">
                                            <div class="pl-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName">Productie</div>
                                                    <div class="col-xs-6 fieldValue">{{ certificate.animals.c.production }}</div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row mb-1">
                                        <div class="col-xs-6">
                                            <div class="pr-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName">Rasstatus/Klasse</div>
                                                    <div class="col-xs-6 fieldValue">{{ certificate.animals.c.breedType }}</div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-6">
                                            <div class="pl-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName">Blindfactor</div>
                                                    <div class="col-xs-6 fieldValue">{{ certificate.animals.c.blindnessFactor }}</div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row mb-1">
                                        <div class="col-xs-6">
                                            <div class="pr-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName">Geboortedatum</div>
                                                    <div class="col-xs-6 fieldValue">{{ certificate.animals.c.dateOfBirth }}</div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-6">
                                            <div class="pl-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName">Predicaat</div>
                                                    <div class="col-xs-6 fieldValue">{{ certificate.animals.c.predicate }}</div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row mb-5">
                                        <div class="col-xs-6">
                                            <div class="pr-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName">Geboorteland</div>
                                                    <div class="col-xs-6 fieldValue">{{ certificate.animals.c.countryOfBirth }}</div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-6">
                                            <div class="pl-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName">Scrapiegenotype</div>
                                                    <div class="col-xs-6 fieldValue">{{ certificate.animals.c.scrapie }}</div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    {# Third block #}
                                    <div class="row mb-1">
                                        <div class="col-xs-12 blockHeader">
                                            <h3 class="display-inline"><b>FOKWAARDEN / NAUWKEURIGHEID</b></h3>
                                        </div>
                                    </div>

                                    <div class="row mb-1">
                                        <div class="col-xs-6">
                                            <div class="pr-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName text-nowrap">Totaal geb. p. worp</div>
                                                    <div class="col-xs-6 fieldValue text-nowrap">
                                                        {% if not certificate.animals.c.breedValues.total_born.isEmpty %}
                                                            {{ certificate.animals.c.breedValues.total_born.value~' / '~ certificate.animals.c.breedValues.total_born.accuracy~'%' }}
                                                        {% endif %}
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-xs-6">
                                            <div class="pl-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName text-nowrap">Vetdikte 20 weken</div>
                                                    <div class="col-xs-6 fieldValue">
                                                        {% if not certificate.animals.c.breedValues.fat_thickness3.isEmpty %}
                                                            {{ certificate.animals.c.breedValues.fat_thickness3.value~' / '~ certificate.animals.c.breedValues.fat_thickness3.accuracy~'%' }}
                                                        {% endif %}
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row mb-1">
                                        <div class="col-xs-6">
                                            <div class="pr-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName text-nowrap">Doodgeb. p. worp</div>
                                                    <div class="col-xs-6 fieldValue text-nowrap">
                                                        {% if not certificate.animals.c.breedValues.still_born.isEmpty %}
                                                            {{ certificate.animals.c.breedValues.still_born.value~' / '~ certificate.animals.c.breedValues.still_born.accuracy~'%' }}
                                                        {% endif %}
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-xs-6">
                                            <div class="pl-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName">Kop</div>
                                                    <div class="col-xs-6 fieldValue">
                                                        {% if not certificate.animals.c.breedValues.skull.isEmpty %}
                                                            {{ certificate.animals.c.breedValues.skull.value~' / '~ certificate.animals.c.breedValues.skull.accuracy~'%' }}
                                                        {% endif %}
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row mb-1">
                                        <div class="col-xs-6">
                                            <div class="pr-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName">Vroegrijpheid</div>
                                                    <div class="col-xs-6 fieldValue">
                                                        {% if not certificate.animals.c.breedValues.early_fertility.isEmpty %}
                                                            {{ certificate.animals.c.breedValues.early_fertility.value~' / '~ certificate.animals.c.breedValues.early_fertility.accuracy~'%' }}
                                                        {% endif %}
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-xs-6">
                                            <div class="pl-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName">Ontwikkeling</div>
                                                    <div class="col-xs-6 fieldValue">
                                                        {% if not certificate.animals.c.breedValues.progress.isEmpty %}
                                                            {{ certificate.animals.c.breedValues.progress.value~' / '~ certificate.animals.c.breedValues.progress.accuracy~'%' }}
                                                        {% endif %}
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row mb-1">
                                        <div class="col-xs-6">
                                            <div class="pr-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName">Tussenlamtijd</div>
                                                    <div class="col-xs-6 fieldValue">
                                                        {% if not certificate.animals.c.breedValues.birth_interval.isEmpty %}
                                                            {{ certificate.animals.c.breedValues.birth_interval.value~' / '~ certificate.animals.c.breedValues.birth_interval.accuracy~'%' }}
                                                        {% endif %}
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-xs-6">
                                            <div class="pl-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName">Bespiering</div>
                                                    <div class="col-xs-6 fieldValue">
                                                        {% if not certificate.animals.c.breedValues.muscularity.isEmpty %}
                                                            {{ certificate.animals.c.breedValues.muscularity.value~' / '~ certificate.animals.c.breedValues.muscularity.accuracy~'%' }}
                                                        {% endif %}
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row mb-1">
                                        <div class="col-xs-6">
                                            <div class="pr-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName">Aflamgemak</div>
                                                    <div class="col-xs-6 fieldValue">
                                                        {% if not certificate.animals.c.breedValues.birth_delivery_progress.isEmpty %}
                                                            {{ certificate.animals.c.breedValues.birth_delivery_progress.value~' / '~ certificate.animals.c.breedValues.birth_delivery_progress.accuracy~'%' }}
                                                        {% endif %}
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-xs-6">
                                            <div class="pl-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName">Evenredigheid</div>
                                                    <div class="col-xs-6 fieldValue">
                                                        {% if not certificate.animals.c.breedValues.proportion.isEmpty %}
                                                            {{ certificate.animals.c.breedValues.proportion.value~' / '~ certificate.animals.c.breedValues.proportion.accuracy~'%' }}
                                                        {% endif %}
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row mb-1">
                                        <div class="col-xs-6">
                                            <div class="pr-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName">Geboortegewicht</div>
                                                    <div class="col-xs-6 fieldValue">
                                                        {% if not certificate.animals.c.breedValues.birth_weight.isEmpty %}
                                                            {{ certificate.animals.c.breedValues.birth_weight.value~' / '~ certificate.animals.c.breedValues.birth_weight.accuracy~'%' }}
                                                        {% endif %}
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-xs-6">
                                            <div class="pl-1">
                                                <div class="row bg-white">
                                                    <div class="col-xs-6 fieldName">Type</div>
                                                    <div class="col-xs-6 fieldValue">
                                                        {% if not certificate.animals.c.breedValues.exterior_type.isEmpty %}
                                                            {{ certificate.animals.c.breedValues.exterior_type.value~' / '~ certificate.animals.c.breedValues.exterior_type.accuracy~'%' }}
                                                        {% endif %}
                                                    </div>
                                                </div>
                                            </div>
