                                        <div class="row">
                                            <div class="col-xs-1">
                                                <b>KOP</b>
                                            </div>
                                            <div class="col-xs-1">
                                                <b>ONT</b>
                                            </div>
                                            <div class="col-xs-1">
                                                <b>BES</b>
                                            </div>
                                            <div class="col-xs-1">
