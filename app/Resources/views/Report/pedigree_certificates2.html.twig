                                    <div class="col-xs-6">
                                        <div class="pl-1">
                                            <div class="row bg-white">
                                                <div class="col-xs-7">Type</div>
                                                <div class="col-xs-5">{{ certificate.animals.c.breedValues.muscle_thickness.value~' / '~ certificate.animals.c.breedValues.muscle_thickness.accuracy~'%' }}</div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row mb-1">
                                    <div class="col-xs-6">
                                        <div class="pr-1">
                                            <div class="row bg-white">
                                                <div class="col-xs-7">Geboortegemak</div>
                                                <div class="col-xs-5">{{ certificate.animals.c.breedValues.birth_progress.value~' / '~ certificate.animals.c.breedValues.birth_progress.accuracy~'%' }}</div>
                                            </div>
                                        </div>
                                    </div>

                                    {# MISSING (leg_work?) #}
                                    <div class="col-xs-6">
                                        <div class="pl-1">
                                            <div class="row bg-white">
                                                <div class="col-xs-7">Beenwerk</div>
                                                <div class="col-xs-5">{{ certificate.animals.c.breedValues.muscle_thickness.value~' / '~ certificate.animals.c.breedValues.muscle_thickness.accuracy~'%' }}</div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row mb-1">
                                    <div class="col-xs-6">
                                        <div class="pr-1">
                                            <div class="row bg-white">
                                                <div class="col-xs-7">8-weken gewicht</div>
                                                <div class="col-xs-5">{{ certificate.animals.c.breedValues.weight_at8weeks.value~' / '~ certificate.animals.c.breedValues.weight_at8weeks.accuracy~'%' }}</div>
                                            </div>
