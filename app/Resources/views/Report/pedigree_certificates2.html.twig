                                        <div class="col-xs-8">
                                            {% for output in certificate.animals.c.indexes.indexBreed.stars_output %}
                                                {% if output == 'FULL' %}
                                                    <span><i class="fas fa-star"></i></span>
                                                {% elseif output == 'HALF' %}
                                                    <span><i class="fas fa-star-half-alt"></i></span>
                                                {% elseif output == 'EMPTY' %}
                                                    <span><i class="far fa-star"></i></span>
                                                {% endif %}
                                            {% endfor %}
                                            {{ '  '~certificate.animals.c.indexes.indexBreed.value~' / '~certificate.animals.c.indexes.indexBreed.accuracy~'%' }}
                                        </div>
                                    {% endif %}
                                </div>

