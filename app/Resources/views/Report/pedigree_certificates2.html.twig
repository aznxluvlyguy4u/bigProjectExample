                                                {% if output == 'FULL' %}
                                                    <span><i class="fas fa-star"></i></span>
                                                {% elseif output == 'HALF' %}
                                                    <span><i class="fas fa-star-half-alt"></i></span>
                                                {% elseif output == 'EMPTY' %}
                                                    <span><i class="far fa-star"></i></span>
                                                {% endif %}
                                            {% endfor %}
                                            {{ '  '~certificate.animals.c.indexes.indexExterior.value~' / '~certificate.animals.c.indexes.indexExterior.accuracy~'%' }}
