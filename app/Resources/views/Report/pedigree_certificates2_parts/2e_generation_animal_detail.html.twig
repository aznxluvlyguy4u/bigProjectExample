<div class="row">
    <div class="col-xs-12 bg-green rounded-top">
        <h2 class="display-inline"><b>{{ title | raw }} - </b></h2><h4 class="display-inline">{{ animal.section_type }}</h4>
    </div>
</div>

<div class="row">
    <div class="col-xs-12 bordered rounded-bottom">
        <div class="px-2 py-1">
            <div class="row mb-1">

                <div class="col-xs-6">
                    <div class="pr-1">
                        <div class="row bg-white-stripe">
                            <div class="col-xs-5 fieldName text-nowrap">Uniek levensnr.</div>
                            <div class="col-xs-7 fieldValue text-nowrap">{{ animal.uln }}</div>
                        </div>
                    </div>
                </div>
                <div class="col-xs-6">
                    <div class="pl-1">
                        <div class="row bg-white-stripe">
                            <div class="col-xs-5 fieldName text-nowrap">Stamboeknr.</div>
                            <div class="col-xs-7 fieldValue text-nowrap">{{ animal.pedigree }}</div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row mb-1">
                <div class="col-xs-6">
                    <div class="pr-1">
                        <div class="row bg-white-stripe">
                            <div class="col-xs-5 fieldName text-nowrap">Ras</div>
                            <div class="col-xs-7 fieldValue text-nowrap">{{ animal.breedCode }}</div>
                        </div>
                    </div>
                </div>
                <div class="col-xs-6">
                    <div class="pl-1">
                        <div class="row bg-white-stripe">
                            <div class="col-xs-5 fieldName text-nowrap">Productie</div>
                            <div class="col-xs-7 fieldValue text-nowrap">{{ animal.production }}</div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row mb-1">
                <div class="col-xs-6">
                    <div class="pr-1">
                        <div class="row bg-white-stripe">
                            <div class="col-xs-5 fieldName text-nowrap">Rasstatus</div>
                            <div class="col-xs-7 fieldValue text-nowrap">{{ animal.breedType }}</div>
                        </div>
                    </div>
                </div>
                <div class="col-xs-6">
                    <div class="pl-1">
                        <div class="row bg-white-stripe">
                            <div class="col-xs-5 fieldName text-nowrap">Blindfactor</div>
                            <div class="col-xs-7 fieldValue text-nowrap">{{ animal.blindnessFactor }}</div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row mb-1">
                <div class="col-xs-6">
                    <div class="pr-1">
                        <div class="row bg-white-stripe">
                            <div class="col-xs-5 fieldName text-nowrap">Geb. datum</div>
                            <div class="col-xs-7 fieldValue text-nowrap">{{ animal.dateOfBirth }}</div>
                        </div>
                    </div>
                </div>
                <div class="col-xs-6">
                    <div class="pl-1">
                        <div class="row bg-white-stripe">
                            <div class="col-xs-5 fieldName text-nowrap">Predicaat</div>
                            <div class="col-xs-7 fieldValue text-nowrap">{{ animal.predicate }}</div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row mb-1">
                <div class="col-xs-6">
                    <div class="pr-1">
                        <div class="row bg-white-stripe">
                            <div class="col-xs-5 fieldName text-nowrap">Geboren als</div>
                            <div class="col-xs-7 fieldValue text-nowrap">{{ animal.nLing }}</div>
                        </div>
                    </div>
                </div>
                <div class="col-xs-6">
                    <div class="pl-1">
                        <div class="row bg-white-stripe">
                            <div class="col-xs-5 fieldName text-nowrap">Scrapiegenotype</div>
                            <div class="col-xs-7 fieldValue text-nowrap">{{ animal.scrapie }}</div>
                        </div>
                    </div>
                </div>

            </div>

            <div class="row bg-white-stripe mb-1">
                <div class="fieldName text-nowrap col-25">Fokker</div>
                <div class="fieldValue text-nowrap col-25">{{ animal.breederName }}</div>
            </div>

            {# SECOND block INDEXES #}
            <div class="row mb-1">
                <div class="col-xs-12 blockHeader">
                    <h3 class="display-inline"><b>INDEX / NAUWKEURIGHEID</b></h3>
                </div>
            </div>

            <div class="row mb-1">
                <div class="col-xs-6">
                    <div class="pr-1">
                        <div class="row bg-white-stripe mh-15">
                            <div class="col-xs-4 fieldName text-nowrap">Vaderindex</div>
                            {% if not animal.indexes.indexFather.isEmpty %}
                                <div class="col-xs-5 pl-5 fieldValue stars text-nowrap">
                                    {% for output in animal.indexes.indexFather.stars_output %}
                                        {% if output == "FULL" %}
                                            <img src="{{ fullStar }}" class="star-compact" alt="">
                                        {% elseif output == "HALF" %}
                                            <img src="{{ halfStar }}" class="star-compact" alt="">
                                        {% elseif output == "EMPTY" %}
                                            <img src="{{ emptyStar }}" class="star-compact" alt="">
                                        {% endif %}
                                    {% endfor %}
                                </div>
                                <div class="col-xs-3 fieldValue text-nowrap">
                                    {{ animal.indexes.indexFather.value~'/'~animal.indexes.indexFather.accuracy }}
                                </div>
                            {% endif %}
                        </div>
                    </div>
                </div>
                <div class="col-xs-6">
                    <div class="pl-1">
                        <div class="row bg-white-stripe mh-15">
                            <div class="col-xs-4 fieldName text-nowrap">Exterieurindex</div>
                            {% if not animal.indexes.indexExterior.isEmpty %}
                                <div class="col-xs-5 pl-5 fieldValue stars text-nowrap">
                                    {% for output in animal.indexes.indexExterior.stars_output %}
                                        {% if output == "FULL" %}
                                            <img src="{{ fullStar }}" class="star-compact" alt="">
                                        {% elseif output == "HALF" %}
                                            <img src="{{ halfStar }}" class="star-compact" alt="">
                                        {% elseif output == "EMPTY" %}
                                            <img src="{{ emptyStar }}" class="star-compact" alt="">
                                        {% endif %}
                                    {% endfor %}
                                </div>
                                <div class="col-xs-3 fieldValue text-nowrap">
                                    {{ animal.indexes.indexExterior.value~'/'~animal.indexes.indexExterior.accuracy }}
                                </div>
                            {% endif %}
                        </div>
                    </div>
                </div>
            </div>

            <div class="row mb-1">
                <div class="col-xs-6">
                    <div class="pr-1">
                        <div class="row bg-white-stripe mh-15">
                            <div class="col-xs-4 fieldName text-nowrap">Moederindex</div>
                            {% if not animal.indexes.indexMother.isEmpty %}
                                <div class="col-xs-5 pl-5 fieldValue stars text-nowrap">
                                    {% for output in animal.indexes.indexMother.stars_output %}
                                        {% if output == "FULL" %}
                                            <img src="{{ fullStar }}" class="star-compact" alt="">
                                        {% elseif output == "HALF" %}
                                            <img src="{{ halfStar }}" class="star-compact" alt="">
                                        {% elseif output == "EMPTY" %}
                                            <img src="{{ emptyStar }}" class="star-compact" alt="">
                                        {% endif %}
                                    {% endfor %}
                                </div>
                                <div class="col-xs-3 fieldValue text-nowrap">
                                    {{ animal.indexes.indexMother.value~'/'~animal.indexes.indexMother.accuracy }}
                                </div>
                            {% endif %}
                        </div>
                    </div>
                </div>
                <div class="col-xs-6">
                    <div class="pl-1">
                        <div class="row bg-white-stripe mh-15">
                            <div class="col-xs-4 fieldName text-nowrap">Rasindex</div>
                            {% if not animal.indexes.indexBreed.isEmpty %}
                                <div class="col-xs-5 pl-5 fieldValue stars text-nowrap">
                                    {% for output in animal.indexes.indexBreed.stars_output %}
                                        {% if output == "FULL" %}
                                            <img src="{{ fullStar }}" class="star-compact" alt="">
                                        {% elseif output == "HALF" %}
                                            <img src="{{ halfStar }}" class="star-compact" alt="">
                                        {% elseif output == "EMPTY" %}
                                            <img src="{{ emptyStar }}" class="star-compact" alt="">
                                        {% endif %}
                                    {% endfor %}
                                </div>
                                <div class="col-xs-3 fieldValue text-nowrap">
                                    {{ animal.indexes.indexBreed.value~'/'~animal.indexes.indexBreed.accuracy }}
                                </div>
                            {% endif %}
                        </div>
                    </div>
                </div>
            </div>


            {# THIRD block EXTERIEURKEURING #}
            <div class="row mb-1">
                <div class="col-xs-12 blockHeader">
                    <h3 class="display-inline"><b>EXTERIEURKEURING&nbsp;&nbsp;&nbsp;&nbsp;</b></h3><p class="fieldValue display-inline">{% if not animal.inspectionDate is empty %}{{ 'KEURDATUM '~animal.inspectionDate }}{% endif %}</p>
                </div>
            </div>

            <div class="row mb-1">
                <div class="col-xs-12">
                    <div class="row blockHeader-alt">
                        <div class="col-xs-1 pl-7px">
                            <h3 class="display-inline"><b>KOP</b></h3>
                        </div>
                        <div class="col-xs-1 pl-7px">
                            <h3 class="display-inline"><b>ONT</b></h3>
                        </div>
                        <div class="col-xs-1 pl-7px">
                            <h3 class="display-inline"><b>BES</b></h3>
                        </div>
                        <div class="col-xs-1 pl-7px">
                            <h3 class="display-inline"><b>EVE</b></h3>
                        </div>
                        <div class="col-xs-1 pl-7px">
                            <h3 class="display-inline"><b>TYP</b></h3>
                        </div>
                        <div class="col-xs-1 pl-7px">
                            <h3 class="display-inline"><b>BEE</b></h3>
                        </div>
                        <div class="col-xs-1 pl-7px">
                            <h3 class="display-inline"><b>VAC</b></h3>
                        </div>
                        <div class="col-xs-1 pl-7px">
                            <h3 class="display-inline"><b>ALG</b></h3>
                        </div>
                        <div class="col-xs-1 pl-7px">
                            <h3 class="display-inline"><b>SHT</b></h3>
                        </div>
                        <div class="col-xs-1 pl-7px">
                            <h3 class="display-inline"><b>LGT</b></h3>
                        </div>
                        <div class="col-xs-1 pl-7px">
                            <h3 class="display-inline"><b>BDP</b></h3>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row bg-white-stripe">
                <div class="col-xs-12">
                    <div class="row">
                        <div class="col-xs-1 fieldValue">
                            {{ animal.skull }}
                        </div>
                        <div class="col-xs-1 fieldValue">
                            {{ animal.development }}
                        </div>
                        <div class="col-xs-1 fieldValue">
                            {{ animal.muscularity }}
                        </div>
                        <div class="col-xs-1 fieldValue">
                            {{ animal.proportion }}
                        </div>
                        <div class="col-xs-1 fieldValue">
                            {{ animal.type }}
                        </div>
                        <div class="col-xs-1 fieldValue">
                            {{ animal.legWork }}
                        </div>
                        <div class="col-xs-1 fieldValue">
                            {{ animal.fur }}
                        </div>
                        <div class="col-xs-1 fieldValue">
                            {{ animal.generalAppearance }}
                        </div>
                        <div class="col-xs-1 fieldValue">
                            {{ animal.height }}
                        </div>
                        <div class="col-xs-1 fieldValue">
                            {{ animal.torsoLength }}
                        </div>
                        <div class="col-xs-1 fieldValue">
                            {{ animal.breastDepth }}
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>